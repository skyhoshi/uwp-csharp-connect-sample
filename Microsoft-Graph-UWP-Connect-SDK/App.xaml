    <Application
    x:Class="Microsoft_Graph_UWP_Connect_SDK.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Microsoft_Graph_UWP_Connect_SDK"
    RequestedTheme="Light">

    <Application.Resources>

        <!-- Add your client id here. -->
        <x:String x:Key="ida:ClientID"></x:String>
        <!-- Add your return URL here. -->
        <x:String x:Key="ida:ReturnUrl"></x:String>
        <x:String x:Key="ida:AADInstance">https://login.chinacloudapi.cn/</x:String>

    </Application.Resources>

    
</Application>
